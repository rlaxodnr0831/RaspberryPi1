//Arduino LED 1,2 ON,OFF Serial Monitoring (Use Arduino IDE)

int led1 = 13;
int led2 = 12;

void setup() {
  pinMode(led1, OUTPUT);
  pinMode(led2, OUTPUT);
  Serial.begin(9600);
}

void loop() {
  digitalWrite(led1, HIGH);
  delay(500);
  Serial.println(led1);

  digitalWrite(led2, HIGH);
  delay(500);
  Serial.println(led2);
  
  digitalWrite(led1, LOW);
  delay(500);
  digitalWrite(led2, LOW);
  delay(500);  
}
